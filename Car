package Sep22.week1.day1;

public class Car {
	
	public void applyBrake()
	{
		System.out.println("applyingBrake");
	}
	public void applyHonk()
	{
		System.out.println("applyingHorn");
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Car Honda = new Car();
		
		Honda.applyBrake();
		Honda.applyHonk();
		

	}

}
